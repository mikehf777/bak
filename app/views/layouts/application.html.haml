!!!
%html
	%head
		= content_tag(:title, title(@title))
		= favicon_link_tag 'favicon.ico'
		= stylesheet_link_tag 'application', :media => 'all'
		= csrf_meta_tags
	%body
		= render 'layouts/header'
		#wrapper
			#content
				= render 'layouts/navigation'
				= yield
		= render 'layouts/footer'
		= javascript_include_tag 'application'